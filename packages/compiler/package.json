{
    "name": "@azoth-web/compiler",
    "version": "1.1.0",
    "description": "Azoth JSX to JavaScript + HTML compiler",
    "keywords": [
        "azoth compiler",
        "azoth-compiler",
        "azoth jsx compiler",
        "azoth-jsx-compiler"
    ],
    "files": [
        "src/!(*test*).js"
    ],
    "author": "Marty Nelson",
    "license": "MIT",
    "type": "module",
    "main": "./src/index.js",
    "exports": {
        ".": {
            "import": {
                "default": "./src/index.js"
            }
        }
    },
    "scripts": {
        "test": "vitest",
        "start": "vite"
    },
    "dependencies": {
        "acorn": "^8.11.3",
        "acorn-jsx": "^5.3.2",
        "astring": "^1.8.6",
        "html-void-elements": "^3.0.0",
        "install": "^0.13.0",
        "is-valid-var-name": "^1.0.2",
        "npm": "^10.4.0",
        "rev-hash": "^4.1.0"
    },
    "devDependencies": {
        "vitest": "^1.2.2"
    }
}