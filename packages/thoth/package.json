{
    "name": "@azoth-web/thoth",
    "version": "1.6.1",
    "description": "Azoth JSX to JavaScript + HTML compiler",
    "author": "Marty Nelson",
    "license": "MIT",
    "keywords": [
        "azoth compiler",
        "azoth-compiler",
        "azoth jsx compiler",
        "azoth-jsx-compiler"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/azoth-web/azoth.git",
        "directory": "packages/thoth"
    },
    "type": "module",
    "main": "./src/index.js",
    "files": [
        "src/!(*test*).js"
    ],
    "exports": {
        ".": {
            "import": {
                "default": "./src/index.js"
            }
        }
    },
    "scripts": {
        "test": "vitest",
        "start": "vite"
    },
    "dependencies": {
        "acorn": "^8.11.3",
        "acorn-jsx": "^5.3.2",
        "astring": "^1.8.6",
        "html-void-elements": "^3.0.0",
        "is-valid-var-name": "^1.0.2",
        "rev-hash": "^4.1.0"
    },
    "devDependencies": {
        "install": "^0.13.0",
        "npm": "^10.4.0",
        "source-map": "^0.7.4",
        "vitest": "^1.3.0"
    }
}