{
    "cSpell.words": [
        "astring",
        "autohid",
        "azothjs",
        "backquote",
        "befcf",
        "dbaeumer",
        "donot",
        "espree",
        "IIFE",
        "inlines",
        "nsbsp",
        "onattribute",
        "onclosetag",
        "oncomment",
        "oninit",
        "onopentag",
        "onopentagname",
        "ontext",
        "quasis",
        "templatize",
        "tfaf",
        "tmpl"
    ],
    "eslint.experimental.useFlatConfig": true,
    "eslint.format.enable": true,
    "eslint.enable": true,
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "files.exclude": {
        "**/dist": true,
        "**/xnode_modules": true
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
}