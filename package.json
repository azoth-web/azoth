{
    "name": "azoth",
    "version": "1.0.1",
    "description": "Azoth JSX hypermedia",
    "author": "Marty Nelson",
    "license": "MIT",
    "keywords": [
        "azoth",
        "azoth-jsx",
        "azothjsx",
        "azoth jsx",
        "javascript framework",
        "javascript library",
        "js library",
        "js framework",
        "ui library",
        "ui framework",
        "hypermedia"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/azoth-web/azoth.git"
    },
    "type": "module",
    "main": "index.js",
    "files": [
        "lib/*.js"
    ],
    "exports": {
        "./composition": "./lib/composition.js",
        "./runtime": "./lib/runtime.js",
        "./vite-plugin": "./lib/vite-plugin.js"
    },
    "scripts": {
        "start": "pnpm -F sandbox start",
        "check": "pnpm update && pnpm test",
        "update": "pnpm update -r",
        "test": "CI=true pnpm test -r -- --passWithNoTests",
        "test:jsonic": "pnpm -F jsonic test",
        "test:runtime": "pnpm -F runtime test",
        "test:compiler": "pnpm -F compiler test",
        "test:test-utils": "pnpm -F test-utils test",
        "test:plugin": "pnpm -F vite-plugin test",
        "lint": "eslint core"
    },
    "devDependencies": {
        "@vitest/browser": "^1.3.1",
        "eslint": "^8.57.0",
        "globals": "^13.24.0",
        "happy-dom": "^13.6.2",
        "vite": "^5.1.4",
        "vite-plugin-inspect": "^0.8.3",
        "vitest": "^1.3.1"
    },
    "packageManager": "^pnpm@8.15.1",
    "dependencies": {
        "@azoth-web/compiler": "workspace:^",
        "@azoth-web/runtime": "workspace:^",
        "@azoth-web/vite-plugin": "workspace:^"
    }
}