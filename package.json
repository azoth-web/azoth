{
    "name": "azoth-monorepo",
    "version": "1.0.0",
    "description": "",
    "type": "module",
    "main": "index.js",
    "scripts": {
        "start": "pnpm -F sandbox start",
        "check": "pnpm update && pnpm test",
        "update": "pnpm update -r",
        "test": "CI=true pnpm test -r -F @azoth-web/*",
        "test:jsonic": "pnpm -F jsonic test",
        "test:maya": "pnpm -F maya test",
        "test:test-utils": "pnpm -F test-utils test",
        "test:plugin": "pnpm -F vite-plugin-azoth test",
        "lint": "eslint .",
        "publish": "pnpm -F maya publish --access=public && pnpm -F thoth publish --access=public && pnpm -F vite-plugin-azoth publish --access=public"
    },
    "keywords": [],
    "author": "",
    "license": "MIT",
    "devDependencies": {
        "@vitest/browser": "^1.3.1",
        "eslint": "^8.57.0",
        "globals": "^13.24.0",
        "happy-dom": "^13.5.3",
        "vite": "^5.1.4",
        "vite-plugin-inspect": "^0.8.3",
        "vitest": "^1.3.1"
    },
    "packageManager": "^pnpm@8.15.1"
}